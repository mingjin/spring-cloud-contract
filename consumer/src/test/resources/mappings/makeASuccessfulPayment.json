{
  "request" : {
    "urlPath": "/portal",
    "method" : "GET",
    "queryParameters": {
      "order_no": {
        "matches": "[0-9]+"
      },
      "charset": {
        "equalTo": "GBK"
      },
      "seller_email": {
        "matches": "[0-9a-zA-Z]+?@[0-9]+?.com"
      },
      "merchant_ID": {
        "matches": "[0-9]+"
      },
      "isApp": {
        "equalTo": "web"
      },
      "notify_url": {
        "matches": "^(https?)://.*?$"
      },
      "return_url": {
        "matches": "^(https?)://.*?$"
      },
      "payment_type": {
        "equalTo": "1"
      },
      "paymethod": {
        "equalTo": "bankPay"
      },
      "service": {
        "equalTo": "online_pay"
      },
      "total_fee": {
        "matches": "[0-9]+.[0-9]+"
      },
      "title": {
        "matches": ".*"
      },
      "body": {
        "matches": ".*"
      },
      "sign": {
        "matches": "[0-9a-zA-Z]+"
      },
      "sign_type": {
        "equalTo": "MD5"
      }
    }
  },
  "response" : {
    "status" : 200,
    "body" : "{\"status\":\"SUCCESS\", \"amount\": \"124\"}",
    "headers" : {
      "Content-Type" : "application/json;charset=UTF-8"
    }
  }
}